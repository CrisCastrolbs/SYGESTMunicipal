@model SYGESTMunicipal.Areas.OFIM.Models.Consulta
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_LayoutOFIM.cshtml";
}

<div class="row">
    <div class="col-md-12 col-lg-12">
        <h3 class="text-light" style="text-align:center;
                              background-color:darkslategray;width:100%;">
            Eliminar una Consulta
        </h3>
    </div>
</div>
<br />
<form method="post" asp-action="Delete">
    <div class="border backgroundWhite">
        <div class="form-group row">
            <div class="col-md-2">
                <label asp-for="ConsultaId" class="col-form-label"></label>
            </div>
            <div class="col-md-5">
                <input asp-for="ConsultaId" class=" form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-md-2">
                <label asp-for="TipoConsultaId" class="col-form-label"></label>
            </div>
            <div class="col-md-5">
                <input asp-for="TipoConsultaId" class=" form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-md-2">
                <label asp-for="PersonaOFIMId" class="col-form-label"></label>
            </div>
            <div class="col-md-5">
                <input asp-for="PersonaOFIMId" class=" form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_DeleteAndBackToListButton" model="Model.ConsultaId" />
            </div>
        </div>
    </div>
</form>