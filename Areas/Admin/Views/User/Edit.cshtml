@model SYGESTMunicipal.Areas.Admin.Models.ApplicationUser
@using SYGESTMunicipal.Areas.Admin.Models
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<h2 class="text-info">Editar Usuario</h2>
<br />
<form method="post" asp-action="Edit">
    <div class="border backgroundWhite">
        <div asp-validation summary="ModelOnly" class="text-danger"></div>

        <div class="form-group row">
            <div class="col-md-2">
                <label asp-for="Id" class="col-form-label"></label>
            </div>
            <div class="col-md-5">
                <input asp-for="Id" class="form-control" />
            </div>
            <span asp-validation-for="Id" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Name" class="col-form-label"></label>
            </div>
            <div class="col-md-5">
                <input asp-for="Name" class="form-control" />
            </div>
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="LastName1" class="col-form-label"></label>
            </div>
            <div class="col-md-5">
                <input asp-for="LastName1" class="form-control" />
            </div>
            <span asp-validation-for="LastName1" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="LastName2" class="col-form-label"></label>
            </div>
            <div class="col-md-5">
                <input asp-for="LastName2" class="form-control" />
            </div>
            <span asp-validation-for="LastName2" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Address" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Address" class="form-control" />
            </div>
            <span asp-validation-for="Address" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="District" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="District" class="form-control" />
            </div>
            <span asp-validation-for="District" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Canton" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Canton" class="form-control" />
            </div>
            <span asp-validation-for="Canton" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Province" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Province" class="form-control" />
            </div>
            <span asp-validation-for="Province" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="PhoneNumber" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="PhoneNumber" class="form-control" />
            </div>
            <span asp-validation-for="PhoneNumber" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_EditAndBackToListButton" model="Model.Id" />
            </div>
        </div>
    </div>
</form>
