@using SYGESTMunicipal.Utility
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/login/assets/img/apple-icon.png">

    <link rel="icon" type="image/png" href="~/login/assets/img/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>
        Login
    </title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <!-- CSS Files -->
    <link href="~/login/assets/css/material-kit.css" rel="stylesheet" />

    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="~/login/assets/demo/demo.css" rel="stylesheet" />
</head>
<body class="login-page sidebar-collapse">
    <header>
        <nav class="navbar navbar-transparent navbar-color-on-scroll fixed-top navbar-expand-lg" color-on-scroll="100" id="sectionsNav">
            <div class="container">

                <div class="navbar-translate">
                    <a class="navbar-brand" asp-area="Home" asp-controller="Home" asp-action="Index" rel="tooltip" data-original-title="Regresar a Sygest">
                        Bienvenido
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="navbar-toggler-icon"></span>
                        <span class="navbar-toggler-icon"></span>
                        <span class="navbar-toggler-icon"></span>
                    </button>
                </div>

                <div class="collapse navbar-collapse">
                    <ul class="navbar-nav ml-auto">

                        <li class="nav-item">
                            <a class="nav-link" rel="tooltip" title="" data-placement="bottom" asp-area="Identity" asp-page="/Account/Login" target="_blank" data-original-title="Iniciar Sesión" rel="nofollow">
                                <i class="fa fa-sign-in"></i>
                            </a>

                        </li>
                        <li class="nav-item">

                            <a class="nav-link" rel="tooltip" title="" data-placement="bottom" href="http://www.munihojancha.com/" target="_blank" data-original-title="Nuestro Sitio web" rel="nofollow">
                                <i class="fa fa-external-link"></i>
                            </a>
                        </li>
                        <li class="nav-item">

                            <a class="nav-link" rel="tooltip" title="" data-placement="bottom" href="mailto:%69%6efo%40muniho%6aan%63%68a.%63om" target="_blank" data-original-title="Contáctanos" rel="nofollow">
                                <i class="fa fa-envelope-open-o"></i>
                                
                            </a>

                        </li>
                        <li class="nav-item">
                            <a class="nav-link" rel="tooltip" title="" data-placement="bottom" href="https://www.facebook.com/MUNICIPALIDAD-DE-HOJANCHA-218380324851885/" target="_blank" data-original-title="Síguenos en Facebook" rel="nofollow">
                                <i class="fa fa-facebook-square"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" rel="tooltip" title="" data-placement="bottom" href="https://www.instagram.com/explore/locations/256658138352395/municipalidad-hojancha/" target="_blank" data-original-title="Síguenos en Instagram" rel="nofollow">
                                <i class="fa fa-instagram"></i>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" rel="tooltip" title="" data-placement="bottom" href="https://www.google.com/maps/place/Municipalidad+de+Hojancha/10.0585518,-85.4189673,18z/data=!4m5!3m4!1s0x8f9fbab7c3d45245:0xb2000651b89c1e5b!8m2!3d10.0587462!4d-85.4178777?hl=es" target="_blank" data-original-title="Ubícanos" rel="nofollow">
                                <i class="fa fa-location-arrow"></i>
                            </a>
                        </li>
                        @if (User.IsInRole(SD.ManagerUser) || (User.IsInRole(SD.Guest) || (User.IsInRole(SD.AdminOFGA) || (User.IsInRole(SD.AdminOFIM) || (User.IsInRole(SD.AdminOFPA) || (User.IsInRole(SD.CollManager) || (User.IsInRole(SD.FrontDeskUser))))))))
                        {
                            <li class="nav-item">
                                <a class="nav-link" rel="tooltip" id="profile" asp-page="./Index" style="color:white;text-shadow: 2px 2px 5px #000" data-placement="bottom" asp-area="Identity"   data-original-title="Mi Perfil">
                                    <i class="fa fa-user-circle "></i>
                                </a>
                            </li>

                            @*<li class="nav-item">
                                <a class="nav-link" rel="tooltip"  id="email" asp-page="./Email" style="color:white;text-shadow: 2px 2px 5px #000" data-placement="bottom" asp-area="Identity"   data-original-title="Cambiar Email">
                                    <i class="fa fa-envelope-o"></i>
                                </a>
                            </li>*@

                            <li class="nav-item">
                                <a class="nav-link" rel="tooltip" id="change-password" asp-page="./ChangePassword" style="color:white;text-shadow: 2px 2px 5px #000" data-placement="bottom" asp-area="Identity"  data-original-title="Cambiar Contraseña">
                                    <i class="fa fa-lock"></i>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" rel="tooltip"  id="personal-data" asp-page="./PersonalData" data-placement="bottom" asp-area="Identity" data-original-title="Datos Personales" >
                                    <i class="fa fa-download"></i>
                                </a>
                            </li>

                           
                               

                        }
                    </ul>
                </div>

            </div>
        </nav>

    </header>
    <div class="show fullscreen">
        <main role="main">
            @RenderBody()
        </main>
    </div>



    <!--   Core JS Files   -->
    <script src="~/login/assets/js/core/jquery.min.js" type="text/javascript"></script>
    <script src="~/login/assets/js/core/popper.min.js" type="text/javascript"></script>
    <script src="~/login/assets/js/core/bootstrap-material-design.min.js" type="text/javascript"></script>
    <script src="~/login/assets/js/plugins/moment.min.js"></script>
    <!--	Plugin for the Datepicker, full documentation here: https://github.com/Eonasdan/bootstrap-datetimepicker -->
    <script src="~/login/assets/js/plugins/bootstrap-datetimepicker.js" type="text/javascript"></script>
    <!--  Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/ -->
    <script src="~/login/assets/js/plugins/nouislider.min.js" type="text/javascript"></script>
    <!--  Google Maps Plugin    -->
    <!-- Control Center for Material Kit: parallax effects, scripts for the example pages etc -->
    <script src="~/login/assets/js/material-kit.js?v=2.0.7" type="text/javascript"></script>
    @section Scripts {

    }

</body>

</html>